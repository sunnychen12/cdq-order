.layui-tab-disabled {
  border-color: #e6e6e6;
  position: relative;
  left: 0;
  height: 40px;
  white-space: nowrap;
  font-size: 0;
  border-bottom-width: 1px;
  border-bottom-style: solid;
  transition: all .2s;
  -webkit-transition: all .2s;
}
.layui-tab-card > .layui-tab-disabled {
  background-color: #f2f2f2;
}
.layui-tab-card > .layui-tab-disabled li {
  margin-right: -1px;
  margin-left: -1px;
}
.layui-tab-card > .layui-tab-disabled .layui-this {
  background-color: #fff;
}
.layui-tab-card > .layui-tab-disabled .layui-this:after {
  border-top: none;
  border-width: 1px;
  border-color: #e6e6e6;
  border-bottom-color: #fff;
}
.layui-tab-disabled li {
  display: inline-block;
  vertical-align: middle;
  font-size: 14px;
  transition: all .2s;
  -webkit-transition: all .2s;
  position: relative;
  line-height: 40px;
  min-width: 65px;
  padding: 0 15px;
  text-align: center;
  cursor: pointer;
}
.layui-tab-disabled .layui-this {
  color: #000;
}
.layui-tab-disabled .layui-this:after {
  position: absolute;
  left: 0;
  top: 0;
  content: '';
  width: 100%;
  height: 41px;
  border-width: 1px;
  border-style: solid;
  border-bottom-color: #fff;
  border-radius: 2px 2px 0 0;
  box-sizing: border-box;
  pointer-events: none;
}
