<script type="text/javascript">
layui.use(['form','jqueryUI','cityselect'], function(){
  var form = layui.form;

  //多选菜单
  setTimeout(function() {
    form.render('checkbox');
  }, 50);

  $('[data-role="city-select"]').cxSelect({
    selects: ['prov', 'city', 'dist']
  })
  .on('click', '.layui-select-title', function(event) {
    $(this).next('dl').css('max-height', 200);
  });

  //省市区
  form.on('select(cityselect)', function(data){
    /*
      data.elem //得到select原始DOM对象
      data.value //得到被选中的值
      data.othis //得到美化后的DOM对象
    */

    var $self=$(data.elem);
    $self.trigger('change');

    var $all=$('[lay-filter="cityselect"]');
    var $prov=$('.prov[lay-filter="cityselect"]');
    var $city=$('.city[lay-filter="cityselect"]');

    if($prov.attr('lay-verify') || (!$prov.attr('lay-verify') && $prov.val()!='' ) ){

      if( $self.hasClass('prov') ){
        if( !$city.prop('disabled') ){
          $city.attr('lay-verify', 'verify-control');
        }
        else{
          $city.removeAttr('lay-verify');
        }

        if(!$self.attr('lay-verify') && $self.val()!=''){
          $all.removeClass('layui-form-danger');
        }
      }
      else if( $self.hasClass('city') ){
        var $dist=$('.dist[lay-filter="cityselect"]');
        if( !$dist.prop('disabled') ){
          $dist.attr('lay-verify', 'verify-control');
        }
        else{
          $dist.removeAttr('lay-verify');
        }
      }
    }
    else{
      $all.removeAttr('lay-verify');

      if($self.hasClass('prov')){
        $all.removeClass('layui-form-danger');
      }
    }
    
    form.render('select');
  }); 

  form.render('select');

  //排序
  $('[data-role="sort-order"]').sortable({
    containment: '[data-role="sort-tbl-box"]',
    placeholder: "sort-highlight",
    connectWith: '[data-role="sort-order"]',
    handle: '[data-role="drop-holder"]',
    forcePlaceholderSize: true,
    zIndex: 999999,
    revert:100,
    helper:function(event, element){
      //debugger
      $(element).children().each(function(index, el) {
        $(this).width($(this).width()+30);
      });
      return element;
    },
    stop: function( event, ui ) {
      var $item=ui.item;
      $item.children().each(function(index, el) {
        $(this).removeAttr('style');
      });
    },
    update: function( event, ui ) {
      var $sender=ui.sender;
      var $item=ui.item;
      
    }
  }).disableSelection();

  
  
});
</script>
