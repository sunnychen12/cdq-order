}
@media (max-width: 767px) {
  .navbar-header {
    margin-bottom: 15px;
  }
  .header {
    padding-bottom: 5px;
  }
  .site-header-nav {
    clear: both;
  }
  .site-header-nav .navbar-right > li > a {
    font-size: 16px;
